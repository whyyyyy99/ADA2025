{
  "steps": [
     {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "entrypoint": "gcloud",
      "args": [
        "container",
        "clusters",
	    "create",
	    "$_CLUSTER_NAME",
	    "--project",
        "$PROJECT_ID",
        "--region",
        "us-central1",
        "--tier",
        "standard",
        "--no-enable-basic-auth",
        "--cluster-version",
        "1.32.1-gke.1357001",
        "--release-channel",
		"regular",
        "--machine-type",
        "e2-medium",
        "--image-type",
		"COS_CONTAINERD",
		"--disk-type",
		"pd-balanced",
		"--disk-size",
		"50",
		"--scopes",
		"https://www.googleapis.com/auth/cloud-platform",
		"--num-nodes",
		"1",
		"--enable-ip-alias",
		"--enable-ip-access",
		"--addons",
		"HorizontalPodAutoscaling,HttpLoadBalancing,GcePersistentDiskCsiDriver",
      ],
      "id": "cluster-creation"
    }
  ],
  "options": {
  "logging": "CLOUD_LOGGING_ONLY"
  }
}